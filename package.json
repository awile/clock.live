{
  "name": "react-static-example-blank",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "yarn run typecheck && yarn run lint && NODE_ENV=production react-static build",
    "analyze": "react-static build --analyze",
    "serve": "serve dist -p 3000",
    "lint": "eslint src/",
    "typecheck": "tsc --project ./tsconfig.json",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "moment-timezone": "^0.5.32",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^7.15.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-config-react-tools": "^1.1.7",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "moment-timezone-data-webpack-plugin": "^1.3.0",
    "react-static": "^7.4.2",
    "react-static-plugin-reach-router": "^7.4.2",
    "react-static-plugin-sass": "^7.3.0",
    "react-static-plugin-sitemap": "^7.4.2",
    "react-static-plugin-source-filesystem": "^7.4.2",
    "react-static-plugin-typescript": "^7.4.2",
    "serve": "^11.1.0",
    "typescript": "^4.1.3",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
